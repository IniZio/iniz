{
  "name": "@iniz/react",
  "version": "0.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/IniZio/iniz.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "tsc --declaration --emitDeclarationOnly --declarationDir ./dist/types -w & vite build --watch",
    "build": "tsc --declaration --emitDeclarationOnly --declarationDir ./dist/types && vite build",
    "lint": "eslint"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-custom": "*",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsconfig": "*",
    "tsup": "^6.2.3",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0"
  },
  "dependencies": {
    "@iniz/core": "*",
    "proxy-state-tree": "^6.3.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "react": "17.x || 18.x"
  }
}
