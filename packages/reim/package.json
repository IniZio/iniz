{
  "name": "reim",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/reim.cjs.js",
  "module": "dist/reim.es.js",
  "unpkg": "dist/reim.js",
  "jsdelivr": "dist/reim.js",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "bili --no-babel.babelrc --watch",
    "build": "npm run build:dist",
    "build:dist": "rimraf dist && bili --no-babel.babelrc"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "semicolon": false,
    "esnext": true,
    "space": 2,
    "envs": [
      "node",
      "browser",
      "es6",
      "serviceworker"
    ],
    "globals": [
      "it"
    ],
    "rules": {
      "import/no-unassigned-import": 0,
      "capitalized-comments": 0,
      "quote-props": [
        "warn",
        "consistent"
      ],
      "react/button-has-type": 0
    }
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.10.0",
    "xo": "^0.21.1"
  },
  "dependencies": {
    "auto-bind": "^1.2.1",
    "immer": "^1.5.0",
    "lodash": "^4.17.10"
  }
}
