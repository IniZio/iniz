{
  "name": "reim",
  "version": "3.0.0",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "reim",
    "react",
    "store",
    "hoc",
    "immer"
  ],
  "main": "dist/reim.cjs.js",
  "module": "dist/reim.es.js",
  "unpkg": "dist/reim.js",
  "jsdelivr": "dist/reim.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "bili --no-babel.babelrc --watch",
    "build": "npm run build:dist && cpx \"src/immer.{d.ts,js.flow}\" dist",
    "build:dist": "rimraf dist && bili --no-babel.babelrc"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": "xo-react",
    "semicolon": false,
    "esnext": true,
    "space": 2,
    "envs": [
      "node",
      "browser",
      "es6",
      "serviceworker"
    ],
    "globals": [
      "it"
    ],
    "rules": {
      "import/no-unassigned-import": 0,
      "capitalized-comments": 0,
      "quote-props": [
        "warn",
        "consistent"
      ],
      "react/button-has-type": 0
    }
  },
  "devDependencies": {
    "@types/auto-bind": "^1.2.0",
    "@types/eslint": "^4.16.6",
    "@types/event-emitter": "^0.3.3",
    "@types/lodash": "^4.14.123",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.2.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.10.0",
    "xo": "^0.21.1"
  },
  "dependencies": {
    "auto-bind": "^1.2.1",
    "event-emitter": "^0.3.5",
    "fast-deep-equal": "^2.0.1",
    "immer": "8",
    "is-plain-object": "^3.0.0"
  },
  "gitHead": "419591f08507517258271f77077dc7a1d5c1f7af"
}
